<set xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Brightness Fade Animation -->
    <alpha
        android:duration="1250"
        android:fromAlpha="0.0"
        android:toAlpha="1.0"
        />

    <!-- Translation Animation
